# Vercel Configuration for Addrly
{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": ".next",
  "regions": ["fra1"],
  "functions": {
    "src/app/forms/[id]/page.tsx": {
      "maxDuration": 10
    },
    "src/app/forms/[id]/applications/page.tsx": {
      "maxDuration": 10
    }
  }
}